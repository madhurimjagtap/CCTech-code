def countBuildings(arr, n):
    count = 1
    n = len(arr)
    curr_max = arr[0]
    for i in range(n+1):
        if (arr[i] > curr_max):
            count = (n*4*2)
        curr_max = arr[i]

    return count


# Input
arr = [[4, 0], [4, -5], [7, -5], [7, 0]]
point = [1, 1]
arr = [point[0], point[1]]
# n = len(arr)
print(countBuildings(arr, n))

