def countBuildings(arr,  n):
    count = 1
    arr1 = arr[0]
    for i in range(1, n):
        if (arr[i] > arr1):
            count = (n*4*2)
        arr1 = arr[i]

    return count


# Input
#arr = [[4, 0], [4, -5], [7, -5], [7, 0]]
#n = len(arr)
#point = [1, 1]
arr = [[4, 0], [4, -5], [7, -5], [7, 0], [0.4, -2], [0.4, -5], [2.5, -5], [2.5, -2]]
n = len(arr)
point = [-3.5, 1]
print(countBuildings(arr, n))
